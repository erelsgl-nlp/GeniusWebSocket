		<param name = "rules_table">original_dirt.od_rules</param>
		<param name = "limit_number_of_rules">20</param>
		<!--  
			SELECT LHS.description, RHS.description 
			FROM original_dirt.od_easyfirst_templates LHS 
			JOIN original_dirt.od_rules ON(LHS.id=left_element_id) 
			JOIN original_dirt.od_easyfirst_templates RHS ON(RHS.id=right_element_id) 
			WHERE LHS.description like '%v:arrest:v%' AND RHS.description like '%v:capture:v%'; ; 
		-->
	</module>



