	<module name="unaryBinc">
		<param name = "database_driver">com.mysql.jdbc.Driver</param>
		<param name = "database_url">jdbc:mysql://$MYSQL/distsim?user=db_readonly</param>

		<!-- ncd_all use Binc similarity -->
		<param name = "templates_table">unary_easyfirst_templates_ncd_all</param>
		<param name = "rules_table">unary_rules_ncd_all</param> 

		<!-- nc_all use Lin similarity. 
		<param name = "templates_table">unary_easyfirst_templates_nc_all</param>
		<param name = "rules_table">unary_rules_nc_all</param>  -->

		<!-- K=20 is the best, according to Idan's COLING-2008 paper -->
		<param name = "limit_number_of_rules">20</param>  
		<param name = "limit_number_of_rules_per_hypothesis">true</param>
		<!--  
SELECT CONCAT(LEFT(description,1),RIGHT(description,1)) AS type, description FROM distsim.unary_easyfirst_templates_ncd_all
GROUP BY 1;

SELECT LHS.description, RHS.description, unary_rules_ncd_all.score
FROM distsim.unary_easyfirst_templates_ncd_all LHS 
JOIN distsim.unary_rules_ncd_all ON(LHS.id=left_element_id) 
JOIN distsim.unary_easyfirst_templates_ncd_all RHS ON(RHS.id=right_element_id) 
WHERE LHS.description like '%v:arrest:v%' AND RHS.description like '%v:capture:v%'
ORDER BY LHS.description, score DESC; 
		-->
	</module>


