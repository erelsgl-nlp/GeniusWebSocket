	<module name="origdirt">
		<param name = "database_driver">com.mysql.jdbc.Driver</param>
		<param name = "database_url">jdbc:mysql://$MYSQL/original_dirt?user=db_readonly</param>
		<param name = "templates_table">original_dirt.od_easyfirst_templates</param>
